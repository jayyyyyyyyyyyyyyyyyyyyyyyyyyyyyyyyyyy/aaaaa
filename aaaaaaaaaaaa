local function CNOXC_fake_script() -- SandhurstExploits.Drills 
	local script = Instance.new('LocalScript', SandhurstExploits)

	script.Parent.Title.elements.DrillsStart.MouseButton1Click:Connect(function()
		local hostName = script.Parent.Title.elements.HostName.Text
		local Host = game.Players:FindFirstChild(hostName)
		local hostCharacter = Host.Character
		local hostTorsoTouched = hostCharacter.Torso.Touched
	
		local Player = script.Parent.Parent.Parent
		local Character = Player.Character
	
		Host.Chatted:Connect(function(Chat)
			if Chat == "SQUAD," then
				script.Parent.SQUAD.Value = true
				wait(7.5)
				script.Parent.SQUAD.Value = false
			end
			if Chat == "LEFT," and script.Parent.SQUAD.Value == true then
				script.Parent.LEFT.Value = true
				wait(15)
				script.Parent.LEFT.Value = false
			end
			if Chat == "RIGHT," and script.Parent.SQUAD.Value == true then
				script.Parent.RIGHT.Value = true
				wait(13)
				script.Parent.RIGHT.Value = false
			end
			if Chat == "ABOUT," and script.Parent.SQUAD.Value == true then
				script.Parent.ABOUT.Value = true
				wait(11)
				script.Parent.ABOUT.Value = false
			end
			--LEFT INCLINE
			if Chat == "INCLINE!" and script.Parent.SQUAD.Value == true and script.Parent.LEFT.Value == true then
				script.Parent.LEFTINCLINE.Value = true
				wait(1)
				script.Parent.LEFTINCLINE.Value = false
				script.Parent.SQUAD.Value = false
				script.Parent.LEFT.Value = false
			--RIGHT INCLINE
			elseif Chat == "INCLINE!" and script.Parent.SQUAD.Value == true and script.Parent.RIGHT.Value == true then
				script.Parent.RIGHTINCLINE.Value = true
				wait(1)
				script.Parent.RIGHTINCLINE.Value = false
				script.Parent.SQUAD.Value = false
				script.Parent.RIGHT.Value = false
			end
			--LEFT TURN
			if Chat == "TURN!" and script.Parent.SQUAD.Value == true and script.Parent.LEFT.Value == true then
				script.Parent.LEFTTURN.Value = true
				wait(1)
				script.Parent.LEFTTURN.Value = false
				script.Parent.SQUAD.Value = false
				script.Parent.LEFT.Value = false
			--RIGHT TURN
			elseif Chat == "TURN!" and script.Parent.SQUAD.Value == true and script.Parent.RIGHT.Value == true then
				script.Parent.RIGHTTURN.Value = true
				wait(1)
				script.Parent.RIGHTTURN.Value = false
				script.Parent.SQUAD.Value = false
				script.Parent.RIGHT.Value = false
			--ABOUT TURN
			elseif Chat == "TURN!" and script.Parent.SQUAD.Value == true and script.Parent.ABOUT.Value == true then
				script.Parent.ABOUTTURN.Value = true
				wait(1)
				script.Parent.ABOUTTURN.Value = false
				script.Parent.SQUAD.Value = false
				script.Parent.ABOUT.Value = false
				
				--Turning
			end
		end)
	end)
end
coroutine.wrap(CNOXC_fake_script)()
